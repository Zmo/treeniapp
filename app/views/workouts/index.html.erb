<h1>Listing workouts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Type</th>
    </tr>
  </thead>

  <tbody>
    <% @workouts.each do |workout| %>
      <tr>
        <td><%= link_to workout.title, workout %></td>
        <td><%= workout.type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New workout', new_workout_path %>

<br>
